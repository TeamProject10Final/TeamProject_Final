<?xml version="1.0" encoding="utf-8"?>
<layer-list
    xmlns:android="http://schemas.android.com/apk/res/android">
<item
    android:top="1dp"
    android:left="1dp"
    android:right="1dp"
    android:bottom="1dp">
    <shape
        android:shape="rectangle">
        <corners
            android:radius="10dp"/>
        <stroke
            android:width="2dp"
            android:color="#C5C5C5"/>
    </shape>
</item>
</layer-list>